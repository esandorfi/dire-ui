    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "^3.2.0",
    "nuxt": "^3.17.5"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.30.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
